    "@nestjs/swagger": "^5.2.1",
    "@nestjs/throttler": "^2.0.1",
    "@nestjs/typeorm": "^8.0.3",
    "base58check": "^2.0.0",
    "bitcoinjs-lib": "^6.0.2",
    "bs58check": "^2.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "ecpair": "^2.0.1",
    "ethers": "^5.6.4",
    "nest-commander": "^2.4.0",
